<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="Farmer_Registration_TaskFlow">
    <default-activity>Farmer_Registration</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>FileManagementBean</managed-bean-name>
      <managed-bean-class>view.FileManagement</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <view id="Farmer_Registration">
      <page>/WEB-INF/Fragments/Modules/Clusters_Farmers/Farmer_Registration.jsff</page>
    </view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
